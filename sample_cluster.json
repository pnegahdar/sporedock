{
    "ID": "My Web Cluster",
    "Docker" : {
        "Host" : "myhost.docker.com"
    },
    "Envs": [
        {
            "ID": "Dev",
            "Env": {
                "DB_URL": "dev.db.com",
                "TYPE": "dev"
            }
        },
        {
            "ID": "Prod",
            "Env": {
                "DB_URL": "prod.db.com",
                "TYPE": "prod"
            }
        }
    ],
    "WebApps": [
        {
            "ID": "Demo Flask App",
            "Image": "arturaugusto/flask-sample:latest",
            "InternalPort": 5000,
            "StartupCommand" : "Yo",
            "EnvIDs": ["Dev"],
            "RunMax": false,
            "WebEndpoints": ["test.flask.com"]
        }
    ]
}